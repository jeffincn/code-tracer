{
  "root": true,
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "linebreak-style": "off",
    "camelcase": "off",
    "semi": "off",
    "comma-style": "off",
    "comma-dangle":"off",
    "space-before-function-paren": "off"
   }
}
